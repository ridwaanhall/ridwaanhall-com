{
	"builds": [
        {
            "src": "FlexForge/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.12"
            }
        }
    ],
    "buildCommand": "npm install && npm run build",
	"routes": [
		{
			"src": "/(.*)",
			"dest": "FlexForge/wsgi.py"
		}
	]
}